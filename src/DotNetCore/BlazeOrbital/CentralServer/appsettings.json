{
  "Apps": {
    "ManufacturingHub": {
      "Origin": "https://localhost:7046"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "IdentityServer": {
    "Clients": {
      "ManufacturingHub": {
        "Profile": "SPA",
        "RedirectUri": "https://localhost:7046/authentication/login-callback",
        "LogoutUri": "https://localhost:7046/authentication/logged-out"
      }
    }
  },
  "AllowedHosts": "*"
}
